<html>
<title>Late Junction playlists</title>
<body>
<ol>
<% @episodes.each do |episode| %>
  <li>
    <a href="/<%= episode.id %>/">
      <%= episode.id %> - <%= episode.date %> - <%= episode.presenter.name %>
    </a>:
    <%= episode.playlist_tracks.length %>
  </li>
<% end %>
</ol>
</body>
</html>
